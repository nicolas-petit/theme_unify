<odoo>

<template id="theme_unify.footer_default_link" name="footer_default_link" inherit_id="theme_unify.assets_frontend">
    <xpath expr="//link[last()]" position="after">
        <link name="footer-default" href="/theme_unify/static/src/css/options/footers/footer-default.css" rel="stylesheet" type="text/css"/>
    </xpath>
</template>

<template id="theme_unify.footer_default" name="footer_default" inherit_id="website.layout">
    <footer position="attributes">
        <attribute name="class">footer-default</attribute>
    </footer>
</template>

<template id="theme_unify.footer_default_automatic" name="footer_default_automatic" inherit_id="website.footer_default">
    <div id="footer" position="attributes">
        <attribute name="class">footer container hidden-print</attribute>
    </div>
</template>

<template id="theme_unify.footer_default_custom" name="footer_default_custom" inherit_id="website.footer_custom">
    <div id="footer" position="attributes">
        <attribute name="class">footer container hidden-print oe_structure</attribute>
    </div>
</template>

<template id="theme_unify.footer_default_copyright" name="footer_default_copyright" inherit_id="website.layout_footer_copyright">
    <div class="container mt16 mb8" position="attributes">
        <attribute name="class">copyright container mt16 mb8</attribute>
    </div>
</template>


<template id="theme_unify.footer_v1_link" name="footer_v1_link" inherit_id="theme_unify.footer_default_link" active="False">
    <link name="footer-default" position="replace">
        <link href="/theme_unify/static/src/css/options/footers/footer-v1.css" rel="stylesheet" type="text/css"/>
    </link>
</template>

